[Unit]
Description=FNTX Daily NAV Import from IBKR FlexQuery
After=network.target

[Service]
Type=oneshot
User=info
WorkingDirectory=/home/info/fntx-ai-v1
Environment="PATH=/usr/bin:/bin:/usr/local/bin"
ExecStart=/usr/bin/python3 /home/info/fntx-ai-v1/01_backend/scripts/daily_flex_import.py
StandardOutput=append:/home/info/fntx-ai-v1/logs/daily_import.log
StandardError=append:/home/info/fntx-ai-v1/logs/daily_import.log